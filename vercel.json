{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "redirects": [
      {
        "source": "/(.*)",
        "has": [
          {
            "type": "host",
            "value": "royalnestgroup.in"
          }
        ],
        "destination": "https://royalnestgroup.com/$1",
        "permanent": true
      },
      {
        "source": "/(.*)",
        "has": [
          {
            "type": "host",
            "value": "www.royalnestgroup.in"
          }
        ],
        "destination": "https://royalnestgroup.com/$1",
        "permanent": true
      }
    ],
    "rewrites": [
      {
        "source": "/feedback/:id",
        "destination": "/api/feedback/:id"
      }
    ]
  }
  